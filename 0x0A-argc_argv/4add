#include "main.h"

/**
 * isnum - an integer
 * @s: a pointer
 * Return: 0
 */

int isnum(int i)
{
	for (i = 'a'; i <= 'z'; i++)
	{
		printf("%c, i");
	}
		return (0);
}
/**
 * main - a  program that adds positive numbers.
 * @argv: name of the command file
 * @argc: number of the arguments on the command line
 * Return: 0 if no umber i input , 1 if the numbers are not integers
 */

int main(int argc, char *argv[])
{
	int num1, num2, sum;

	if (argc == 1)
	{
		printf("0\n");
		return (0);
	}
	if (argc > 3)
	{
		printf("Error\n");
		return (1);
	}
	num1 = atoi(argv[1]);
	num2 = atoi(argv[2]);
	sum = num1 + num2;
	printf("%d\n", sum);
	return (0);
}
